    <section class="about_section layout_padding">
      <div class="container">
        <div class="row d-flex justify-content-center">
          <div class="col-md-6">
            <div class="detail-box">
              <div class="heading_container">
                <h2 style="margin:auto; margin-bottom: 20px;">Edit User Kader</h2>
                <form>
                  <div class="form-group">
                    <label for="staff-full-name">Nama Lengkap</label>
                    <input
                      type="text"
                      name="staff-full-name"
                      class="form-control"
                      style="width: 550px"
                      id="staff-full-name"
                      value="Lisa Edelstein"
                    />
                  </div>
                  <div class="form-group">
                    <label for="staff-nip">NIP</label>
                    <input
                      type="number"
                      name="staff-nip"
                      class="form-control"
                      style="width: 550px;"
                      id="staff-nip"
                      value="000000000001"
                    />
                    <div class="form-group">
                      <label for="staff-gender">Jenis Kelamin</label>
                      <select
                      name="staff-gender"
                      class="form-control"
                      style="width: 550px"
                      id="staff-gender">
                      <option value="p">Perempuan</option>
                      <option value="l">Laki-Laki</option>
                      </select>
                    </div>
                  </div>
                  <div class="form-group">
                    <label for="staff-birth-place">Tempat Lahir</label>
                    <input
                      type="text"
                      name="staff-birth-place"
                      class="form-control"
                      style="width: 550px"
                      id="staff-birth-place"
                      value="Jakarta"
                    />
                  </div>
                  <div class="form-group">
                    <label for="staff-birth-date">Tanggal Lahir</label>
                    <input
                      type="date"
                      name="staff-birth-date"
                      class="form-control"
                      style="width: 550px"
                      id="staff-birth-date"
                      value="1980-01-20"
                    />
                  </div>
                  <div class="form-group">
                    <label for="staff-address">Alamat</label>
                    <input
                      type="text"
                      name="staff-address"
                      class="form-control"
                      style="width: 550px"
                      id="staff-address"
                      value="Jl. Sasa no 45 Kebon Jeruk Jakarta"
                    />
                  </div>
                  <div class="form-group">
                    <label for="staff-password">Password</label>
                    <input
                      type="staff-password"
                      name="staff-password"
                      class="form-control"
                      id="staff-password"
                      value="************"
                    />
                  </div>
                  <div class="d-flex justify-content-center p-2 gap-3">
                    <button type="submit" class="btn btn-danger" style="margin-top: 20px;">Delete</button>
                    <button type="submit" class="btn btn-success" style="margin-top: 20px;">Save</button>
                  </div>
                </form>
              </div>
            </div>
          </div>
