    <section class="about_section layout_padding">
      <div class="container">
        <div class="row d-flex justify-content-center">
          <div class="col-md-6">
            <div class="detail-box">
              <div class="heading_container">
                <h2 style="margin-left: 75px">Edit Hasil Pemeriksaan</h2>
                <form style="width: auto" id="medical-report">
                  <div class="form-group">
                    <label for="patient-name">Nama Pasien</label>
                    <select
                      type="text"
                      id="patient-name"
                      class="form-control"
                      name="patientName"
                      style="width: 600px"
                      required>
                    <option value="">Dina</option>
                    <option value="">Mina</option>
                    <option value="">Kina</option>
                    <option value="">Rina</option>

                    </select>
                  </div>
                  <div class="form-group">
                    <label for="patient-height">Tinggi Badan(cm)</label>
                    <input
                      type="number"
                      id="patient-height"
                      class="form-control"
                      name="patient-height"
                      style="width: 600px"
                      value="52.5"
                      required
                    />
                  </div>
                  <div class="form-group">
                    <label for="patient-weight">Berat Badan(kg)</label>
                    <input
                      type="number"
                      id="patient-weight"
                      class="form-control"
                      name="patient-weight"
                      style="width: 600px"
                      value="7.1"
                      required
                    />
                  </div>
                  <div class="form-group">
                    <label for="report">Hasil Pemeriksaan</label>
                    <textarea
                      id="report"
                      class="form-control"
                      name="report"
                      style="resize: none; height: 200px"
                      required
                    >Sehat</textarea>
                  </div>
                  <div class="d-flex justify-content-center p-2">
                    <button type="submit" class="btn btn-success">
                      Submit
                    </button>
                  </div>
                </form>
              </div>
            </div>
          </div>
        </div>
